[
    {
        "key": "ctrl+o",
        "command": "workbench.action.navigateBack",
        "when": "editorTextFocus && !inDebugRepl && vim.useCtrlKeys && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+i",
        "command": "workbench.action.navigateForward",
        "when": "editorTextFocus && !inDebugRepl && vim.useCtrlKeys && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+f",
        "command": "actions.find",
        "when": "editorTextFocus && !inDebugRepl && vim.useCtrlKeys && vim.mode == 'Insert'"
    },
    {
        "key": "ctrl+n",
        "command": "extension.vim_cmd+d",
        "when": "editorTextFocus && !inDebugRepl && !suggestWidgetVisible && vim.useCtrlKeys && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+n",
        "command": "extension.vim_cmd+d",
        "when": "editorTextFocus && !inDebugRepl && !suggestWidgetVisible && vim.useCtrlKeys && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+p",
        "command": "editor.action.moveSelectionToPreviousFindMatch",
        "when": "editorTextFocus && !inDebugRepl && !suggestWidgetVisible && vim.useCtrlKeys && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+x",
        "command": "editor.action.moveSelectionToNextFindMatch",
        "when": "editorTextFocus && !inDebugRepl && !suggestWidgetVisible && vim.useCtrlKeys && vim.mode == 'Visual'"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenNavigatePrevious",
        "when": "inQuickOpen && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenNavigateNext",
        "when": "inQuickOpen && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+c",
        "command": "workbench.action.closeQuickOpen",
        "when": "inQuickOpen && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+[",
        "command": "workbench.action.closeQuickOpen",
        "when": "inQuickOpen && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+c",
        "command": "workbench.action.closeMessages",
        "when": "globalMessageVisible && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+[",
        "command": "workbench.action.closeMessages",
        "when": "globalMessageVisible && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+c",
        "command": "leaveEditorMessage",
        "when": "messageVisible && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+[",
        "command": "leaveEditorMessage",
        "when": "messageVisible && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+shift+c",
        "command": "editor.action.clipboardCopyAction",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+x",
        "command": "editor.action.clipboardCutAction",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+,",
        "command": "editor.action.outdentLines",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+.",
        "command": "editor.action.indentLines",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+shift+up",
        "command": "cursorColumnSelectUp",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+shift+down",
        "command": "cursorColumnSelectDown",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+t",
        "command": "workbench.action.tasks.runTask",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+n",
        "command": "workbench.action.files.newUntitledFile",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+b",
        "command": "workbench.files.action.focusFilesExplorer",
        "when": "editorTextFocus && vim.mode == 'Normal'"
    },
    {
        "key": "escape",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+[",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "explorerViewletFocus && vim.useCtrlKeys"
    },
    {
        "key": "ctrl+c",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "explorerViewletFocus && vim.useCtrlKeys"
    },
    {
        "key": "k",
        "command": "list.focusUp",
        "when": "explorerViewletFocus"
    },
    {
        "key": "j",
        "command": "list.focusDown",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+p",
        "command": "list.focusUp",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+n",
        "command": "list.focusDown",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+d",
        "command": "list.focusPageDown",
        "when": "explorerViewletFocus"
    },
    {
        "key": "ctrl+u",
        "command": "list.focusPageUp",
        "when": "explorerViewletFocus"
    },
    {
        "key": "h",
        "command": "list.collapse",
        "when": "explorerViewletFocus"
    },
    {
        "key": "l",
        "command": "list.expand",
        "when": "explorerViewletFocus"
    },
    {
        "key": "o",
        "command": "list.select",
        "when": "explorerViewletFocus"
    },
    {
        "key": "v",
        "command": "explorer.openToSide",
        "when": "explorerViewletVisible && explorerViewletFocus"
    },
    {
        "key": "r",
        "command": "renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "y",
        "command": "filesExplorer.copy",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "p",
        "command": "filesExplorer.paste",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "d",
        "command": "deleteFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "x",
        "command": "deleteFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    }
]
